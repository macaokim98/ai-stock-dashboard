# Stock Price Dashboard Environment Variables

# Yahoo Finance Configuration (No API key required)
# Yahoo Finance provides free stock data
VITE_USE_YAHOO_FINANCE=true

# Claude API Configuration (for AI analysis)
# Get your API key from: https://console.anthropic.com/
VITE_CLAUDE_API_KEY=your_claude_api_key_here

# Application Configuration
VITE_APP_NAME=Stock Price Dashboard
VITE_APP_VERSION=1.0.0

# Development Settings
VITE_DEBUG_MODE=false
VITE_MOCK_DATA=false

# API Configuration
VITE_API_TIMEOUT=10000
VITE_CACHE_DURATION=300000

# Feature Flags
VITE_ENABLE_AI_ANALYSIS=true
VITE_ENABLE_REAL_TIME=false
VITE_ENABLE_NOTIFICATIONS=false